<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="monkey">
    <sequence name="walk-down" start="0" end="3" loop="true" />
    <sequence name="walk-left"  start="5" end="8" loop="true" />
    <sequence name="walk-right" start="10" end="13" loop="true" />
    <sequence name="walk-up"  start="15" end="18" loop="true" />

</animation>

